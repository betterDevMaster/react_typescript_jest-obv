  "scripts": {
    "dev": "react-scripts start",
    "build": "react-scripts build",
    "test": "cross-env CI=true react-scripts test --env=jest-environment-jsdom-sixteen",
    "test:watch": "react-scripts test --env=jest-environment-jsdom-sixteen",
    "test:coverage": "npm run test -- --coverage",
    "test:related": "npm run test -- --passWithNoTests --findRelatedTests",
    "test:fail_only": "npm run test 2>&1 | grep 'FAIL'",
    "prettier": "prettier --ignore-path .gitignore \"**/*.+(js|jsx|ts|tsx|json)\"",
    "format": "npm run prettier -- --write",
    "check-format": "npm run prettier -- --list-different",
    "lint": "eslint --ignore-path .gitignore --ext .js,.jsx,.ts,.tsx .",
    "check-types": "tsc",
    "validate": "npm run format && npm-run-all --parallel check-types check-format lint test build",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run check-types && lint-staged"
    }
  },
  "lint-staged": {
    "**/*.+(js|jsx|ts|tsx)": [
      "eslint"
    ],
    "**/*.+(js|jsx|ts|tsx|json)": [
      "prettier --write"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!**/__utils__/**",
      "!**/src/index.tsx",
      "!**/serviceWorker.ts",
      "!**/react-app-env.d.ts",
      "!**/*.d.ts"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "@types/axios": "^0.14.0",
    "@types/color": "^3.0.1",
    "@types/croppie": "^2.6.0",
    "@types/css-mediaquery": "^0.1.0",
    "@types/emoji-mart": "^3.0.4",
    "@types/faker": "^4.1.12",
    "@types/jest": "^24.0.0",
    "@types/lodash": "^4.14.168",
    "@types/node": "^12.0.0",
    "@types/ramda": "^0.27.14",
    "@types/react": "^16.9.0",
    "@types/react-beautiful-dnd": "^13.0.0",
    "@types/react-color": "^3.0.4",
    "@types/react-dom": "^16.9.0",
    "@types/react-helmet": "^6.1.0",
    "@types/react-redux": "^7.1.9",
    "@types/react-router-dom": "^5.1.6",
    "@types/react-virtualized": "^9.21.11",
    "@types/styled-components": "^5.1.3",
    "@types/uuid": "^8.3.0",
    "css-mediaquery": "^0.1.2",
    "faker": "^5.1.0",
    "husky": "^4.2.5",
    "jest-canvas-mock": "^2.3.0",
    "jest-environment-jsdom-sixteen": "^1.0.3",
    "lint-staged": "^10.2.13",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.1.1",
    "redux-devtools": "^3.7.0"
  }
}
